var salariobruto,valorhoraaula,htrabmes,pcdescontoINSS,valdescontoINSS,salliq;
valorhoraaula=Number(prompt("digite o valor hora-aula:"));
htrabmes=Number(prompt("digite as horas trabalhadas no mês:"));
pcdescontoINSS=Number(prompt("digite o percentual de desconto:"));
salariobruto=(valorhoraaula*htrabmes);
valdescontoINSS=(psdescontoINSS/100*salariobruto);
salliq=(slariobruto-valdescontoINSS);
alert("o salário líquido: "+salliq)

