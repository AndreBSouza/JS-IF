16

var SM,PERRE,NS;
SM=Number(prompt("digite o salário mensal: "));
PERRE=Number(prompt("digite o percentual de reajuste: "));
NS=SM+(SM*PERRE/100);
alert("Novo salário: "+NS)

17

var A,B,C,Branco,Nulo,eleitores,pervotoA,pervotoB,pervotoC,perBranco,perNulo
A=Number(prompt("digite o número de votos para o candidato A: "));
B=Number(prompt("digite o número de votos para o candidato B: "));
C=Number(prompt("digite o número de votos para o candidato C: "));
Branco=Number(prompt("digite o número de votos brancos: "));
Nulo=Number(prompt("digite o número de votos nulos: "));
eleitores=(A+B+C+Branco+Nulo);
perA=(A*100/eleitores);
perB=(B*100/eleitores);
perC=(C*100/eleitores);
perBranco=(Branco*100/eleitores);
perNulo=(Nulo*100/eleitores);
alert("Total de eleitores: "+eleitores+". Percentual de votos para o candidato A: "+perA+". Percentual de votos para o candidato B: " +perB+". Percentual de votos para o candidato C: "+perC+". Percentual de votos em branco: "+perBranco+". Percentual de votos nulos: "+perNulo)

18

var QLT,VLT,AREA,R,A;
R=Number(prompt("digite o raio: "));
A=Number(prompt("digite a altura: "));
AREA=2*3.14*R*(A+R);
QLT=AREA/15;
VLT=QLT*50;
alert("área dos tanques cilíndricos: "+AREA+", quantidade de latas de tinta necessárias: "+QLT+" e custo para pintar os tanques: R$"+VLT)
