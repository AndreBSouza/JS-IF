11

var A,B,C;
A=Number(prompt("digite o valor de A: "));
B=Number(prompt("digite o valor de B: "));
C=A;
A=B;
alert("A vale: "+A);
B=C;
alert("B vale: "+B)

12

var A,B,C,D;
A=Number(prompt("Digite o valor de A: "));
B=Number(prompt("Digite o valor de B: "));
C=(A+B)*(A-B);
D=(A*A)-(B*B);
alert("C vale: "+C);
alert("D vale: "+C)

13

var A,B,C,D;
A=Number(prompt("digite o valor de A: "));
B=Number(prompt("digite o valor de B: "));
C=Number(prompt("digite o valor de C: "));
D=(A*A)+(B*B)+(C*C)
alert("D vale: "+D)

14

var A,B,C,D;
A=Number(prompt("digite o valor de A: "));
B=Number(prompt("digite o valor de B: "));
C=Number(prompt("digite o valor de C: "));
D=(A+B+C)*(A+B+C);
alert("D é igual a: "+D)

