5

var V,R,A;
R=Number(prompt("digite raio: "));
A=Number(prompt("digite area: "));
V=(Math.PI*R*R*A);
alert("o volume é: "+V)



6

var D,T,V,LU
T=Number(prompt("digite o tempo: "));
V=Number(prompt("digite a velocidade: "));
D=(T*V);
LU=(D/12);
alert("litros usados: "+LU)


7

var P,V,T,TE;
V=Number(prompt("digite Valor: "));
T=Number(prompt("digite Taxa: "));
TE=Number(prompt("digite Tempo: "));
P=(V+(V*T/100)*TE);
alert("prestação: "+P)

8

var V,C,L,A;
C=Number(prompt("digite o comprimento: "));
L=Number(prompt("digite a largura: "));
A=Number(prompt("digite a altura: "));
V=(C*L*A);
alert("o volume é: " + V)

