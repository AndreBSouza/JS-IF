			break;
		case "não":
			alert("ata, xau!")
			break;
	}
	if (A2==A1||A2==A3||A2==A4||A2==B2|A2==C2||A2==D2){
		if (err<3) {
			err++;
			alert("Você errou! Tente novamente!")
		}
		if (err==3) {
			alert("Você errou três vezes! Você perdeu!")
		}
	}
}
